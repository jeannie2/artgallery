    <script src="https://cdn.jsdelivr.net/npm/lodash@4.17.21/lodash.min.js"></script>
    <script>
      window.parseFormData = (formData) => {
        const parsed = {}

        Array.from(formData.entries()).forEach((data) => {
          _.set(parsed, data[0], data[1] || '')
        })

        return parsed
      }
    </script>
    <%- scripts %>
